    "framer-motion": "^10.12.17",
    "html-react-parser": "^3.0.15",
    "next": "^13.4.4",
    "next-safe": "^3.4.1",
    "next-strict-csp": "^1.0.1",
    "nvm": "^0.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-select": "^5.7.3",
    "remark": "^14.0.2",
    "remark-html": "^15.0.2",
    "tailwind-merge": "^1.13.0",
    "url": "^0.11.0",
    "yup": "^1.0.2"
